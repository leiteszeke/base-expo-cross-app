{
  "name": "@leiteszeke/base-expo-cross-app",
  "version": "4.10.9",
  "expoVersion": {
    "android": 1,
    "ios": 1
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "NODE_ENV=development expo start --web",
    "test": "jest e2e/**/*.e2e.js",
    "build:web": "NODE_ENV=production expo export:web",
    "build:android": "NODE_ENV=production eas build -p android",
    "build:ios": "NODE_ENV=production eas build -p ios",
    "submit:android": "NODE_ENV=production eas submit -p android",
    "submit:ios": "NODE_ENV=production eas submit -p ios",
    "update:android": "NODE_ENV=production eas update -p android",
    "update:ios": "NODE_ENV=production eas update -p ios",
    "postinstall": "yarn-dedupe && patch-package",
    "check-types": "tsc -p tsconfig.json --noemit",
    "lint": "yarn eslint && yarn check-types",
    "lint:fix": "eslint ./**/*.ts ./**/*.tsx --fix",
    "prettier:fix": "prettier --write App.tsx ./**/*.ts ./**/*.tsx",
    "prettier:check": "prettier --check App.tsx ./**/*.ts ./**/*.tsx",
    "predeploy": "rm -rf web-build && yarn build:web",
    "release": "vercel --prod --archive=tgz",
    "prerelease": "rm -rf web-build && yarn build:web",
    "deploy:prod": "./deploy.sh production",
    "deploy:qa": "./deploy.sh qa",
    "e2e:build:android:debug": "detox build --configuration android.debug",
    "e2e:build:ios:debug": "detox build --configuration ios.debug",
    "e2e:android:debug": "NODE_ENV=local detox test --configuration android.debug --reuse",
    "e2e:ios:debug": "NODE_ENV=local detox test --configuration ios.debug --reuse",
    "e2e:build:android": "detox build --configuration android.release",
    "e2e:build:ios": "detox build --configuration ios.release",
    "e2e:android": "detox test --configuration android.release --reuse",
    "e2e:ios": "detox test --configuration ios.release --reuse",
    "e2e:web": "start-server-and-test web http-get://localhost:8081 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "maestro:test": "cd e2e/maestro; maestro test login.yaml"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@bothrs/expo-mixpanel-analytics": "^2.0.3",
    "@expo/webpack-config": "~19.0.1",
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/themed": "^1.1.54",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@sentry/react-native": "^5.33.2",
    "consistencss": "^1.7.0",
    "date-fns": "^4.1.0",
    "expo": "^51.0.37",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-image": "^1.13.0",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-router": "^3.5.23",
    "expo-splash-screen": "^0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "^0.25.27",
    "firebase": "^10.14.0",
    "formik": "^2.4.6",
    "i18n-js": "^4.4.3",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.451.0",
    "mixpanel-browser": "^2.55.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.75.4",
    "react-native-date-picker": "^5.0.7",
    "react-native-device-info": "^13.0.0",
    "react-native-event-listeners": "^1.0.7",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-native-web": "~0.19.10",
    "sentry-expo": "^7.2.0",
    "socket.io-client": "^4.8.0",
    "styled-components": "^6.1.13",
    "yup": "^1.4.0",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@config-plugins/detox": "^8.0.0",
    "@types/lodash": "^4.17.10",
    "@types/mixpanel-browser": "^2.50.0",
    "@types/react": "^18.3.11",
    "@types/react-native": "^0.73.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^13.15.0",
    "detox": "^20.27.2",
    "detox-expo-helpers": "^0.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.12.0",
    "expo-detox-hook": "^1.0.10",
    "husky": "^9.1.6",
    "jest": "^29.4.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "^51.0.4",
    "normalize-css-color": "^1.0.2",
    "patch-package": "^8.0.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^18.3.1",
    "start-server-and-test": "^2.0.8",
    "typescript": "^5.6.3",
    "yarn-dedupe": "^0.2.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true,
  "main": "expo-router/entry"
}
