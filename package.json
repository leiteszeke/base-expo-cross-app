{
  "name": "@leiteszeke/base-expo-cross-app",
  "version": "1.1.0",
  "expoVersion": {
    "android": 1,
    "ios": 1
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "NODE_ENV=development expo start --web",
    "test": "jest e2e/**/*.e2e.js",
    "build:web": "NODE_ENV=production expo export:web",
    "build:android": "NODE_ENV=production eas build -p android",
    "build:ios": "NODE_ENV=production eas build -p ios",
    "submit:android": "NODE_ENV=production eas submit -p android",
    "submit:ios": "NODE_ENV=production eas submit -p ios",
    "update:android": "NODE_ENV=production eas update -p android",
    "update:ios": "NODE_ENV=production eas update -p ios",
    "postinstall": "yarn-dedupe && patch-package",
    "check-types": "tsc -p tsconfig.json --noemit",
    "lint": "yarn eslint && yarn check-types",
    "lint:fix": "eslint ./**/*.ts ./**/*.tsx --fix",
    "prettier:fix": "prettier --write App.tsx ./**/*.ts ./**/*.tsx",
    "prettier:check": "prettier --check App.tsx ./**/*.ts ./**/*.tsx",
    "prepare": "husky install",
    "predeploy": "rm -rf web-build && yarn build:web",
    "release": "vercel --prod --archive=tgz",
    "prerelease": "rm -rf web-build && yarn build:web",
    "deploy:prod": "./deploy.sh production",
    "deploy:qa": "./deploy.sh qa",
    "e2e:build:android:debug": "detox build --configuration android.debug",
    "e2e:build:ios:debug": "detox build --configuration ios.debug",
    "e2e:android:debug": "NODE_ENV=local detox test --configuration android.debug --reuse",
    "e2e:ios:debug": "NODE_ENV=local detox test --configuration ios.debug --reuse",
    "e2e:build:android": "detox build --configuration android.release",
    "e2e:build:ios": "detox build --configuration ios.release",
    "e2e:android": "detox test --configuration android.release --reuse",
    "e2e:ios": "detox test --configuration ios.release --reuse",
    "e2e:web": "start-server-and-test web http-get://localhost:19006 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "maestro:test": "cd e2e/maestro; maestro test"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@bothrs/expo-mixpanel-analytics": "^2.0.3",
    "@expo/webpack-config": "^18.1.3",
    "@gluestack-style/react": "^1.0.8",
    "@gluestack-ui/config": "^1.0.3",
    "@gluestack-ui/themed": "^1.0.8",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@sentry/react-native": "^5.11.1",
    "consistencss": "^1.7.0",
    "date-fns": "^2.30.0",
    "expo": "^49.0.16",
    "expo-application": "^5.4.0",
    "expo-constants": "~14.4.2",
    "expo-device": "^5.6.0",
    "expo-image": "^1.5.1",
    "expo-linking": "^6.0.0",
    "expo-localization": "^14.5.0",
    "expo-splash-screen": "^0.22.0",
    "expo-status-bar": "^1.7.1",
    "expo-updates": "^0.18.17",
    "firebase": "^10.5.2",
    "formik": "^2.4.5",
    "i18n-js": "^4.3.2",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.290.0",
    "mixpanel-browser": "^2.47.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.72.6",
    "react-native-date-picker": "^4.3.3",
    "react-native-device-info": "^10.11.0",
    "react-native-event-listeners": "^1.0.7",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^13.14.0",
    "react-native-web": "^0.19.9",
    "sentry-expo": "^7.1.1",
    "socket.io-client": "^4.7.2",
    "styled-components": "^6.1.0",
    "yup": "^1.3.2",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@config-plugins/detox": "^6.0.0",
    "@types/lodash": "^4.14.200",
    "@types/mixpanel-browser": "^2.47.4",
    "@types/react": "^18.2.33",
    "@types/react-native": "^0.72.5",
    "cypress": "^13.3.3",
    "detox": "^20.13.3",
    "detox-expo-helpers": "^0.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "expo-detox-hook": "^1.0.10",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "^49.0.0",
    "normalize-css-color": "^1.0.2",
    "patch-package": "^8.0.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^2.0.1",
    "typescript": "^5.2.2",
    "yarn-dedupe": "^0.2.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
