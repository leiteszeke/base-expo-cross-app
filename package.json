{
  "name": "@leiteszeke/base-expo-cross-app",
  "version": "1.0.0",
  "expoVersion": {
    "android": 1,
    "ios": 1
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "NODE_ENV=local expo start --web",
    "test": "jest e2e/**/*.e2e.js",
    "build:web": "NODE_ENV=production expo export:web",
    "build:android": "NODE_ENV=production eas build -p android",
    "build:ios": "NODE_ENV=production eas build -p ios",
    "submit:android": "NODE_ENV=production eas submit -p android",
    "submit:ios": "NODE_ENV=production eas submit -p ios",
    "update:android": "NODE_ENV=production eas update -p android",
    "update:ios": "NODE_ENV=production eas update -p ios",
    "postinstall": "yarn-dedupe && patch-package",
    "check-types": "tsc -p tsconfig.json --noemit",
    "lint": "yarn eslint && yarn check-types",
    "lint:fix": "eslint ./**/*.ts ./**/*.tsx --fix",
    "prettier:fix": "prettier --write App.tsx ./**/*.ts ./**/*.tsx",
    "prettier:check": "prettier --check App.tsx ./**/*.ts ./**/*.tsx",
    "prepare": "husky install",
    "predeploy": "rm -rf web-build && yarn build:web",
    "release": "vercel --prod --archive=tgz",
    "prerelease": "rm -rf web-build && yarn build:web",
    "deploy:prod": "./deploy.sh production",
    "deploy:qa": "./deploy.sh qa",
    "e2e:build:android:debug": "detox build --configuration android.debug",
    "e2e:build:ios:debug": "detox build --configuration ios.debug",
    "e2e:android:debug": "NODE_ENV=local detox test --configuration android.debug --reuse",
    "e2e:ios:debug": "NODE_ENV=local detox test --configuration ios.debug --reuse",
    "e2e:build:android": "detox build --configuration android.release",
    "e2e:build:ios": "detox build --configuration ios.release",
    "e2e:android": "detox test --configuration android.release --reuse",
    "e2e:ios": "detox test --configuration ios.release --reuse",
    "e2e:web": "start-server-and-test web http-get://localhost:19006 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "maestro:test": "cd e2e/maestro; maestro test"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@bothrs/expo-mixpanel-analytics": "^2.0.3",
    "@expo/webpack-config": "^18.1.0",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@sentry/react-native": "^5.5.0",
    "consistencss": "^1.7.0",
    "date-fns": "^2.30.0",
    "expo": "^48.0.19",
    "expo-application": "~5.1.1",
    "expo-constants": "~14.2.1",
    "expo-device": "~5.2.1",
    "expo-linking": "~4.0.1",
    "expo-localization": "~14.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "firebase": "^9.22.2",
    "formik": "^2.4.1",
    "i18n-js": "^4.2.3",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.47.0",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.71.10",
    "react-native-device-info": "^10.6.0",
    "react-native-event-listeners": "^1.0.7",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "~3.20.0",
    "react-native-web": "^0.19.4",
    "sentry-expo": "^6.2.1",
    "socket.io-client": "^4.6.2",
    "yup": "^1.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@config-plugins/detox": "^5.0.1",
    "@types/lodash": "^4.14.195",
    "@types/mixpanel-browser": "^2.38.1",
    "@types/react": "^18.2.9",
    "@types/react-native": "^0.72.2",
    "cypress": "^12.14.0",
    "detox": "^20.9.1",
    "detox-expo-helpers": "^0.6.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "expo-detox-hook": "^1.0.10",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-circus": "^29.4.3",
    "jest-expo": "^48.0.0",
    "patch-package": "^7.0.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.3",
    "yarn-dedupe": "^0.2.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
